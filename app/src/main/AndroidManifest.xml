<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aliya.adapter.simple">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DecorAdapterListSimpleActivity" />
        <activity android:name=".activity.DecorAdapterGridSimpleActivity" />
        <activity android:name=".activity.AdapterUnifyDataSimpleActivity" />
        <activity android:name=".activity.AdapterDiffDataSimpleActivity" />
        <activity android:name=".activity.DecorAdapterLoadMoreSimpleActivity" />
        <activity android:name=".activity.OverlaySimpleActivity" />
        <activity android:name=".activity.EmptyPageSimpleActivity"></activity>
    </application>

</manifest>